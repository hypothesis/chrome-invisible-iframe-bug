{
  "name": "Chrome iframe bug test",
  "description": "Test case for https://github.com/hypothesis/client/issues/516",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icons/logo-128.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "icons/logo-38.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": ["src/*"]
}
